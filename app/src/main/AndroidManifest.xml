<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taskill">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Taskill">
        <activity
            android:name=".RegisterActivity"
            android:exported="true" />
        <activity
            android:name=".ui.login.LoginActivity2"
            android:exported="true"
            android:label="@string/title_activity_login2" />
        <activity
            android:name=".MainActivityBot"
            android:exported="true"
            android:label="@string/title_activity_main_bot" />
        <activity
            android:name=".LogInActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
    </application>

</manifest>